<script type="text/javascript" src="https://intelshare.intelink.gov/sites/jfhq-dodin/SiteAssets/SPDialog.js"></script>function dialogPopUp(url) {
        var options = {
            autosize: true,
            showClose: true,
            title: ' ',
            url: url,
        };
        SP.SOD.execute('sp.ui.dialog.js', 'SP.UI.ModalDialog.showModalDialog', options);
   }
</script>
